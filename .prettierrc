{
  
  "overrides": [

    {
      "files": "*.{html,css,js}",
      "options": {
        "parser": "babel"
      }
      
    },
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]

  
  ,
  "plugins": [
    "prettier-plugin-svelte",
    "prettier-plugin-organize-imports",
    "prettier-plugin-tailwindcss",
    "prettier-plugin-html", "prettier-plugin-css"
  ],
  "pluginSearchDirs": false,
  "singleQuote": false,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "arrowParens": "avoid"
}
